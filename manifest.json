{

  "description": "Removes suggested videos from the Youtube interface.",
  "manifest_version": 2,
  "name": "Youtube-No-Suggestions",
  "version": "1.0",
  "homepage_url": "",

  "applications": {
    "gecko": {
      "id": "youtube-no-suggest@msbrown.net",
      "strict_min_version": "45.0"
    }
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "css": ["remove_suggestions.css"]
    }
  ]

}
